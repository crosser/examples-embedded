version = "1.0.0";
description = "Packed types example";

schema Ex_Packed {

    enum E { E0, E1, E2 };
    typedef num<1,4> N;
    typedef N A[3];
    
    packed P {
        E e;
        A a;
    };
    
    struct S {
        E e;
        A a;
    }

    P p;
    S s;
    
    uint8 pSize { readonly };
    uint8 sSize { readonly };

};
